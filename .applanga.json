{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "language": "en",
          "file_format": "gettext_po",
          "path": "",
          "tag": "app:language.po"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "gettext_po",
          "path": "/<language>.json",
          "tag": "app:language.po"
        }
      ]
    }
  }
}