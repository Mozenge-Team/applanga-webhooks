{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "language": "en",
          "file_format": "nested_json",
          "path": "./.react_json_sample/en/translations.json",
          "tag": "github-demo-tag"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "nested_json",
          "path": "./react_json_sample/<language>/translation.json",
          "tag": "github-demo-tag"
        }
      ]
    }
  }
}