{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./.github/workflows/applanga-pull.yml",
          "file_format": "symfony_yaml",
          "tag": "",
          "language": "en"
        },
        {
          "path": "./.react_json_sample/en/translations.json",
          "file_format": "nested_json",
          "tag": "app:language.json",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./.github/workflows/<language>.yml",
          "file_format": "symfony_yaml",
          "tag": "app:language.yaml",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./.react_json_sample/en/<language>.json",
          "file_format": "nested_json",
          "tag": "app:language.json",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}