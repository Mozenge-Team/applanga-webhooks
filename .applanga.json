{
  "app": {
    "base_language": "en-US",
    "push": {
      "source": [
        {
          "language": "en-US",
          "file_format": "react_nested_json",
          "path": "./.react_json_sample/en/translations.json",
          "tag": "app:language.json"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "exclude_languages": [
            "en-US"
          ],
          "file_format": "react_nested_json",
          "path": "./.react_json_sample/en/<language>.json",
          "tag": "app:language.json"
        }
      ]
    }
  }
}